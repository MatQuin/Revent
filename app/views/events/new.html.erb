
<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-4 well well-sm">
            <legend><a href="http://www.jquery2dotnet.com"><i class="glyphicon glyphicon-globe"></i></a></legend>

<form class="form-horizontal">
<fieldset>

<!-- Form Name -->
<legend>Event Form</legend>

<!-- Text input-->
<div class="form-group">
  <label class="control-label" for="event_name">Name</label>

  <input id="event_name" name="event_name" type="text" placeholder="Event Name" class="form-control input-md">


</div>

<!-- Textarea -->
<div class="form-group">
  <label class=" control-label" for="textarea">Description</label>

    <textarea class="form-control" id="textarea" name="textarea"></textarea>

</div>

<!-- Textarea -->
<div class="form-group">
  <label class="control-label" for="textarea">Details and Lineup</label>

    <textarea class="form-control" id="textarea" name="textarea"></textarea>

</div>

<!-- Text input-->
<div class="form-group">
  <label class=" control-label" for="textinput">Start Date</label>

  <input id="textinput" name="textinput" type="text" placeholder="Start Date" class="form-control input-md">
  <span class="help-block">help</span>

</div>

<!-- Text input-->
<div class="form-group">
  <label class="control-label" for="textinput">End Date</label>

  <input id="textinput" name="textinput" type="text" placeholder="End Date" class="form-control input-md">
  <span class="help-block">help</span>
</div>

<!-- Textarea -->
<div class="form-group">
  <label class=" control-label" for="textarea">Recurring Info</label>

    <textarea class="form-control" id="textarea" name="textarea"></textarea>

</div>

<!-- Select Basic -->
<div class="form-group">
  <label class=" control-label" for="selectbasic">Categories</label>

    <select id="selectbasic" name="selectbasic" class="form-control">
      <option value="1">Option one</option>
      <option value="2">Option two</option>
    </select>

</div>

<!-- Select Basic -->
<div class="form-group">
  <label class=" control-label" for="selectbasic">Tags</label>

    <select id="selectbasic" name="selectbasic" class="form-control">
      <option value="1">Option one</option>
      <option value="2">Option two</option>
    </select>
</div>

<!-- Select Basic -->
<div class="form-group">
  <label class="control-label" for="selectbasic">Relevance</label>

    <select id="selectbasic" name="selectbasic" class="form-control">
      <option value="1">Endorsement</option>
      <option value="2">Resource</option>
      <option value="3">Hide</option>
    </select>

</div>

<!-- Multiple Checkboxes -->
<div class="form-group">
  <label class=" control-label" for="checkboxes">Participation Access</label>

  <div class="checkbox">
    <label for="checkboxes-0">
      <input type="checkbox" name="checkboxes" id="checkboxes-0" value="1">
      Paid
    </label>
  <div class="checkbox">
    <label for="checkboxes-1">
      <input type="checkbox" name="checkboxes" id="checkboxes-1" value="2">
      Invite only
    </label>
	</div>
  <div class="checkbox">
    <label for="checkboxes-2">
      <input type="checkbox" name="checkboxes" id="checkboxes-2" value="3">
      Open
    </label>
	</div>
  </div>
</div>

<!-- Select Basic -->
<div class="form-group">
  <label class="control-label" for="selectbasic">Payment Method</label>
    <select id="selectbasic" name="selectbasic" class="form-control">
      <option value="1">In advance</option>
      <option value="2">At door</option>
      <option value="3">Both</option>
    </select>
</div>

<!-- Textarea -->
<div class="form-group">
  <label class=" control-label" for="textarea">Pricing info</label>
    <textarea class="form-control" id="textarea" name="textarea"></textarea>
</div>

</fieldset>
</form>

        </div>
    </div>
