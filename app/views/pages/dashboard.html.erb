    <li>3v3nt8</li>
    <li>3v3nt9</li></div>
    <div class="ns-TabPanel" id="ns-TabPanelC" data-tab-panel="C">
    <li>3v3nt7</li>
    <li>3v3nt8</li>
    <li>3v3nt9</li></div>

  </div><!-- /.ns-TabPanels -->

</section><!-- /.ns-TabsModule -->
